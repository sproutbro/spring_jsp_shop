<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cf.eisp.spring_jsp_shop.repository.NoticeRepository">
    <select id="findAll" resultType="Notice">
        select * from notice0
    </select>

    <select id="find_id" resultType="Notice" parameterType="Integer">
        select * from notice0 where nno = #{nno}
    </select>
</mapper>

